{
    "name": "vinkla/shield",
    "description": "A HTTP basic auth middleware for Laravel",
    "keywords": [
        "auth",
        "basic",
        "http",
        "laravel",
        "middleware",
        "shield"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@doubledip.se"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^7.0",
        "symfony/http-kernel": "^5.0"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^3.0",
        "graham-campbell/testbench": "^5.4",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.0-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Shield\\ShieldServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Shield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Shield\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
