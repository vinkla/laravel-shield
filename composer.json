{
    "name": "vinkla/shield",
    "description": "A HTTP basic auth middleware for Laravel",
    "keywords": [
        "laravel",
        "http",
        "basic",
        "auth",
        "middleware",
        "shield"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.8.*",
        "symfony/http-kernel": "^4.2"
    },
    "require-dev": {
        "graham-campbell/analyzer": "^2.1",
        "graham-campbell/testbench": "^5.2",
        "phpunit/phpunit": "^7.5 || ^8.0"
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.7-dev"
        },
        "laravel": {
            "providers": [
                "Vinkla\\Shield\\ShieldServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Shield\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Shield\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
